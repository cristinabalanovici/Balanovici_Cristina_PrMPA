@{
}
@model IEnumerable<Balanovici_Cristina_PrMPA.Models.CabinetViewModels.ProgramareGroup>
@{
    ViewData["Title"] = "Statistici programari";
}
<h2>Statistici programari</h2>
<table>
    <tr>
        <th>
            Data
        </th>
        <th>
            Numar Programari
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DataProgramare)
            </td>
            <td>
                @item.NrProgramari
            </td>
        </tr>
    }
</table>